{
  "name": "searchrepo",
  "version": "0.9.0",
  "description": "Search Repo",
  "engines": {
    "node": "8.12.0"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "main": "index.js",
  "scripts": {
    "test": "node test.js --env=jsdom",
    "build": "webpack --mode production",
    "heroku-postbuild": "npm install && npm run build",
    "start": "node server"
  },
  "jest": {
    "collectCoverageFrom": [
      "components/**/*.{js,jsx}"
    ],
    "testRegex": "/__tests__/.*\\.(test|spec)\\.js$",
    "testEnvironment": "node",
    "testURL": "http://localhost:8080",
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setupTests.js"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/polyfills.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/config/jest/transform.js",
      "^.+\\.(scss|css)$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|scss|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ],
    "globals": {
      "window": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dearparanoid/SearchRepo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dearparanoid/SearchRepo/issues"
  },
  "homepage": "https://github.com/dearparanoid/SearchRepo#readme",
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.18.0",
    "consolidate": "^0.15.1",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "mustache": "^3.0.1",
    "node-sass": "^4.11.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "styled-components": "^4.1.3",
    "styled-flex-component": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.1.0",
    "jest-enzyme": "^7.0.1",
    "jest-junit-reporter": "^1.1.0",
    "jest-styled-components": "^6.3.1",
    "object-assign": "^4.1.1",
    "react-test-renderer": "^16.8.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  }
}
